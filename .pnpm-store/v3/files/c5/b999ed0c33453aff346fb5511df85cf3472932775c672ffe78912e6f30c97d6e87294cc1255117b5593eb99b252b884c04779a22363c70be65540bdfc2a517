import type { IApi, IRoute } from '@umijs/types';
import type { IDumiOpts } from '../..';
export declare type RouteProcessor = (this: {
    options: IDumiOpts;
    umi: IApi;
    data: Record<string, any>;
}, routes: IRoute[]) => IRoute[];
declare const _default: (routes: IRoute[], opts: IDumiOpts, umi: IApi) => IRoute[];
/**
 * decorator standard umi routes for dumi
 */
export default _default;
