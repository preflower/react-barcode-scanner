/**
 * get demo route name
 * @note  also use this function in CLI, do not use BOM inside
 */
export declare const getDemoRouteName: () => "_demos" | "~demos";
/**
 * get single demo url
 * @param demoId  demo identifier
 * @param htmlSuffix true when `exportStatic: { htmlSuffix: true }`
 */
export declare const getDemoUrl: (demoId: string, htmlSuffix?: boolean) => string;
declare const _default: (demoId: string) => string;
/**
 * hooks for get single demo url
 */
export default _default;
