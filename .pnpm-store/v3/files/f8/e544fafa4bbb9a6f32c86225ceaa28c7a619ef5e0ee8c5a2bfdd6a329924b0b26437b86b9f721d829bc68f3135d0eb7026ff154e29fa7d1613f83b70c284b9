/**
 * transform props base on JSX rule
 * @param props   original props
 */
export declare const formatJSXProps: (props: Record<string, any>) => Record<string, any>;
/**
 * get umi dynamicImport flag
 */
export declare function isDynamicEnable(): boolean;
export declare const encodeHoistImport: (resolvePath: string) => string;
export declare const decodeHoistImport: (str: string, id: string) => string;
export declare const isHoistImport: (str: string) => boolean;
export declare const decodeHoistImportToContent: (str: string) => string;
/**
 * encode import/require statement by dynamicImport, it can be decode to await import statement with chunkName
 */
export declare const CHUNK_ID = "^C^H^U^N^K^";
export declare const encodeImportRequire: (resolvePath: string) => string;
export declare const decodeImportRequire: (str: string, chunkName: string) => string;
export declare const isEncodeImport: (str: string) => boolean;
export declare const decodeImportRequireWithAutoDynamic: (str: string, chunkName: string) => string;
