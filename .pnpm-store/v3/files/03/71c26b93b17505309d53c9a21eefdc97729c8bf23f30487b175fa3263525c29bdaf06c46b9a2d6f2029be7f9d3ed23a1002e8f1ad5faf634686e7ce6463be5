import type { RouteProcessor } from '.';
/**
 * check route path wether prefix locale path
 * @param routePath   route path
 * @param localeName  locale prefix name
 */
export declare function isPrefixLocalePath(routePath: string, localeName: string): boolean;
/**
 * discard locale prefix path from route path
 * @param routePath   route path
 * @param localeName  locale prefix name
 */
export declare function discardLocalePrefix(routePath: string, localeName: string): string;
/**
 * add locale prefix for route path
 * @param routePath   route path
 * @param localeName  locale prefix name
 */
export declare function addLocalePrefix(routePath: string, localeName: string): string;
declare const _default: RouteProcessor;
/**
 * set locale for route
 */
export default _default;
