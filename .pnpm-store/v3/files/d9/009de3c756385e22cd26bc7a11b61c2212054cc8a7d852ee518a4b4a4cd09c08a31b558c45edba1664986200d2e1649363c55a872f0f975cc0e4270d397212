  t.is(convert('autofocus'), 'autoFocus');
  t.is(convert('autoplay'), 'autoPlay');
  t.is(convert('capture'), 'capture');
  t.is(convert('cellpadding'), 'cellPadding');
  t.is(convert('cellspacing'), 'cellSpacing');
