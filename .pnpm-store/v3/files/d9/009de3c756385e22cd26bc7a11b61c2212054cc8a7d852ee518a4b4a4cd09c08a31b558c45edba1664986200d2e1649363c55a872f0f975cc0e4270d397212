  t.is(convert('onsuspend'), 'onSuspend');
  t.is(convert('ontimeupdate'), 'onTimeUpdate');
  t.is(convert('onvolumechange'), 'onVolumeChange');
  t.is(convert('onwaiting'), 'onWaiting');
  t.is(convert('onload'), 'onLoad');
  t.is(convert('onerror'), 'onError');
});

test('convert supported attributes with capital letters', t => {
  t.is(convert('ACCEPT'), 'accept');
  t.is(convert('ACTION'), 'action');
  t.is(convert('classID'), 'classID');
  t.is(convert('CLAss'), 'className');
});

test('bypass unsupported attributes', t => {
  t.is(convert('data-test'), 'data-test');
  t.is(convert('aria-label'), 'aria-label');
  t.is(convert('unsupported-attr'), 'unsupported-attr');
});
