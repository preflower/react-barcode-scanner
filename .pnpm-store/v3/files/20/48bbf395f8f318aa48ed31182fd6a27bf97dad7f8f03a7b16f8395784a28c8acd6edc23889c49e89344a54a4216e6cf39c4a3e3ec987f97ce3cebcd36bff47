import React from 'react';
import type { LinkProps, NavLinkProps } from 'react-router-dom';
/**
 * Link component wrapper for render external link
 * @param Component   original Link component
 */
export declare const LinkWrapper: (Component: React.FC<LinkProps | NavLinkProps>) => ({ to, ...props }: LinkProps | NavLinkProps) => JSX.Element;
declare const _default: ({ to, ...props }: LinkProps<H.LocationState> | NavLinkProps<H.LocationState>) => JSX.Element;
export default _default;
