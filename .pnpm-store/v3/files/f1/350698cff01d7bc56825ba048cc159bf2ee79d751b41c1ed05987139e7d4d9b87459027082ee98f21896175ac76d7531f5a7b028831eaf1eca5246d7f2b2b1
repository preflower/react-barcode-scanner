export declare type PrefersColorValue = 'dark' | 'light' | 'auto';
declare const _default: () => [PrefersColorValue, (val: PrefersColorValue) => void];
/**
 * hook for get/set prefers-color-schema, use to control color mode for theme package
 * @see https://developer.mozilla.org/en-US/docs/Web/CSS/@media/prefers-color-scheme
 */
export default _default;
